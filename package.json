  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "alsatian": "^3.0.1",
    "babel-loader": "^8.0.6",
    "barrelsby": "^2.1.1",
    "rimraf": "^3.0.0",
    "tap-bark": "^2.1.0",
    "terser-webpack-plugin": "^2.1.2",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
